{
  "homepage": "./",
  "build": {
    "appId": "com.lavallee.remoteftc",
    "productName": "Remote FTC",
    "files": [
      "build/**/*",
      "electron/main.ts",
      "package.json"
    ],
    "extends": null,
    "win": {
      "target": [
        "portable"
      ]
    }
  },
  "name": "remoteftc",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.ts",
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "electron-is": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "reconnecting-websocket": "^4.4.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/ react-scripts build",
    "build-electron": "react-scripts build",
    "electron:build:win": "yarn build-electron && electron-builder build --publish never --win --x64",
    "electron:build": "yarn build-electron && electron-builder build --publish never",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.2",
    "concurrently": "^7.0.0",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "wait-on": "^6.0.0"
  }
}
